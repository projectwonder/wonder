<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo wocomponentcontent = "false" class="IMTextAction.java">
		<binding name = "value"/>
		<binding name = "allowBlanks"/>
		<binding name = "action"/>
		
    <validation message = "'action' is a required binding">
      <unbound name = "action"/>
    </validation>
		
    <validation message = "'value' is a required binding">
      <unbound name = "value"/>
    </validation>
	</wo>
</wodefinitions>