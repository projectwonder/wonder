<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wodefinitions>
	<wo class="ERDEditPassword" wocomponentcontent="false">

		<binding name="object"/>
		<binding name="key"/>
		<binding name="passwordConfirmationValidates" defaults="Boolean"/>
		<binding name="length"/>

		<validation message="'object' is a required binding">
			<unbound name="object"/>
		</validation>

		<validation message="'key' is a required binding">
			<unbound name="key"/>
		</validation>
	</wo>
</wodefinitions>